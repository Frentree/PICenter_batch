<?xml version="1.0" encoding="EUC-KR"?>

<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"  "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="update">
	<update id="setNetIp" parameterClass="com.skyun.app.util.database.ibatis.vo.NetTargetVo" >
		UPDATE pi_targets T 
		   SET T.NET_TYPE = #type#
		WHERE 1=1
		   AND T.TARGET_ID = #target_id#
		   AND T.AP_NO = #ap_no#
	</update>
	
</sqlMap>
